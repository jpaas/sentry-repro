{
  "[css]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "[javascriptreact]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[svg]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "[typescriptreact]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "[xml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "files.exclude": {
    "**/.bundle": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.gradle": true,
    "**/.project": true,
    "**/.settings": true,
    "**/build": true,
    "**/coverage": true,
    "**/dist": true,
    "**/node_modules": true,
    "**/tmp": true,
    "**/vendor": true
  },
  "jest.jestCommandLine": "jest-web.config.js",
  "jest.rootPath": "core-react-module",
  "js/ts.implicitProjectConfig.experimentalDecorators": true,
  "relay.autoStartCompiler": true,
  "ruby.codeCompletion": false,
  "ruby.format": "rubocop",
  "ruby.intellisense": "rubyLocate",
  "ruby.lint": {
    "rubocop": true
  },
  "ruby.locate": {
    "exclude": "{**/@(tmp|.*),**/@(tmp|.*)/**,**/cdk.out/**,**/node_modules/**}"
  },
  "ruby.pathToBundler": ".vscode/bundle",
  "ruby.rubocop.configFilePath": ".rubocop.yml",
  "ruby.rubocop.executePath": ".vscode/",
  "ruby.useBundler": true,
  "ruby.useLanguageServer": false,
  "solargraph.commandPath": ".vscode/solargraph",
  "solargraph.useBundler": false,
  "sortier.onSave": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsdk": "node_modules/typescript/lib"
}
